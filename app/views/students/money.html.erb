<% content_for :header do %>
  <script>
    jQuery(function($){
      //alert("ewe");
      $(".date_pick").datepicker();
    })
  </script>
<% end %>
<%= form_tag({:controller => "students", :action => "show_money"}, :method => "get" ) do %>
  <input type="text" name="start_at" value="<%= (Time.now - 1.year).strftime("%Y/%m/%d") %>" class="date_pick"/>
  <input type="text" name="end_at" value="<%= Time.now.strftime("%Y/%m/%d") %>" class="date_pick"/> 
  <br /> 
  <% Incomelevel.all.each do |income| %>
    <%= check_box_tag "income_ids[]", income.id %> <%= income.reason_desc %>        
  <% end %>  
  <%= submit_tag value="send" %>
<% end %>